{
   "author": "Christian Schiller, Leonard Lausen, (Curt Seeling)",
   "background": {
      "scripts": [ "dict.js", "main.js", "background.js" ]
   },
  "browser_action": {
     "default_icon": "images/zhongwen48.png",
     "default_title": "Zhongwen Chinesisch-Deutsch Popup Worterbuch",
     "browser_style": true
  },
   "content_scripts": [ {
      "all_frames": true,
      "match_about_blank": true,
      "js": [ "content.js" ],
      "matches": [ "<all_urls>" ]
   } ],
   "description": "Great tool for learning Chinese. Includes links to Chinese Grammar Wiki. Supports adding words to Skritter. Firefox fork of the Christian Schillers Chrome extension. And English translations replaced by German translations.",
   "icons": {
      "128": "images/zhongwen.png",
      "16": "images/zhongwen16.png",
      "48": "images/zhongwen48.png"
   },
   "homepage_url": "https://github.com/CNurt/zhongwen/tree/german",
   "manifest_version": 2,
   "name": "Zhongwen: Chinesisch-Deutsch Popup Worterbuch",
   "options_ui": {
     "page": "options.html",
     "browser_style": true,
     "chrome_style": true
   },
  "permissions": [ "*://*/*", "contextMenus", "storage", "downloads"],
   "short_name": "Zhongwen Dewen",
  "version": "4.12.1",
  "web_accessible_resources": [ "css/*", "js/*", "images/*" ],
  "applications": {
    "gecko": {
      "id": "{00000000-0000-0000-0000-000000000000}", //I have no idea but it doesn't work without any id
      "strict_min_version": "55.0"
    }
  }
}
